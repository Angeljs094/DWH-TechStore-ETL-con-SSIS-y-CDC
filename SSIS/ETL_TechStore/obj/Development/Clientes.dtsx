          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{CB61A389-962D-4E1C-9ACD-A0F9E7082B98}"
              SQLTask:SqlStatementSource="BEGIN TRANSACTION;&#xA;BEGIN TRY&#xA;    -- 1) Cerrar versiones actuales que cambiaron&#xA;    UPDATE d&#xA;    SET d.FechaFin = GETDATE(),&#xA;        d.EsActual = 0&#xA;    FROM BDS.DimCliente d&#xA;    INNER JOIN ODS.Clientes o ON d.ClienteID = o.ClienteID&#xA;    WHERE d.EsActual = 1&#xA;      AND (&#xA;            d.NombreCompleto &lt;&gt; o.NombreCompleto OR&#xA;            d.Email &lt;&gt; o.Email OR&#xA;            d.Ciudad &lt;&gt; o.Ciudad OR&#xA;            d.Pais &lt;&gt; o.Pais&#xA;          );&#xA;&#xA;    -- 2) Insertar nuevas versiones (nuevos + modificados)&#xA;    INSERT INTO BDS.DimCliente (ClienteID, NombreCompleto, Email, Ciudad, Pais, FechaInicio, FechaFin, EsActual)&#xA;    SELECT &#xA;        o.ClienteID, o.NombreCompleto, o.Email, o.Ciudad, o.Pais,&#xA;        CAST(GETDATE() AS DATE), NULL, 1&#xA;    FROM ODS.Clientes o&#xA;    LEFT JOIN BDS.DimCliente d ON o.ClienteID = d.ClienteID AND d.EsActual = 1&#xA;    WHERE d.ClienteID IS NULL&#xA;       OR d.NombreCompleto &lt;&gt; o.NombreCompleto&#xA;       OR d.Email &lt;&gt; o.Email&#xA;       OR d.Ciudad &lt;&gt; o.Ciudad&#xA;       OR d.Pais &lt;&gt; o.Pais;&#xA;&#xA;    COMMIT;&#xA;END TRY&#xA;BEGIN CATCH&#xA;    ROLLBACK;&#xA;    THROW;&#xA;END CATCH;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
