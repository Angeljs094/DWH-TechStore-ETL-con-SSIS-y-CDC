         * Example of using a File connection manager
         *  object rawConnection = Dts.Connections["Prices.zip"].AcquireConnection(Dts.Transaction);
         *  string filePath = (string)rawConnection;
         *  //Use the connection in some code here, then release the connection
         *  Dts.Connections["Prices.zip"].ReleaseConnection(rawConnection);
         * */
        #endregion


		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		

        public void Main()
        {
            try
            {
                string fromLSN = Dts.Variables["User::LSN_From_Clientes"].Value.ToString();
                string toLSN = Dts.Variables["User::LSN_To_Clientes"].Value.ToString();

                string query;

                if (fromLSN == "FIRST_TIME" || string.IsNullOrEmpty(fromLSN))
                {
                    query = string.Format(@"
                        DECLARE @from_lsn BINARY(10), @to_lsn BINARY(10);
                     -- Obtener el LSN m nimo (primera vez)
                     SET @from_lsn = sys.fn_cdc_get_min_lsn('Fuente.Clientes');
                     SET @to_lsn = CONVERT(BINARY(10), '{0}', 1);
                     -- Validar que hay un rango v lido
                     IF @to_lsn IS NULL OR @from_lsn IS NULL OR @from_lsn > @to_lsn
                     BEGIN
                     -- No hay cambios que procesar
                     SELECT TOP 0
                        ClienteID, NombreCompleto, Email, Ciudad, Pais, __$operation AS CDC_Operation
                     FROM cdc.Fuente_Clientes_CT;
                     END
                     ELSE
                     BEGIN
                        -- Leer todos los cambios desde el inicio
                     SELECT 
                        ClienteID, NombreCompleto, Email, Ciudad, Pais, __$operation AS CDC_Operation
                     FROM cdc.fn_cdc_get_all_changes_Fuente_Clientes(@from_lsn, @to_lsn, 'all')
                     ORDER BY __$start_lsn;
                     END
                        ", toLSN);

                    MessageBox.Show("Primera Ejecucion detectada. Se procesaron todos los cambios historios.", "CDC - Primera ejecucion"
                        , MessageBoxButtons.OK
                        , MessageBoxIcon.Information);

                }
                else
                {
                    query = string.Format(@"
                        DECLARE @from_lsn BINARY(10), @to_lsn BINARY(10);
                SET @from_lsn = CONVERT(BINARY(10), '{0}', 1);
                SET @to_lsn = CONVERT(BINARY(10), '{1}', 1);
                -- Validar que hay un rango v lido
                IF @to_lsn IS NULL OR @from_lsn IS NULL OR @from_lsn >= @to_lsn
                BEGIN
                    -- No hay cambios nuevos
                    SELECT TOP 0
                        ClienteID, NombreCompleto, Email, Ciudad, Pais, __$operation AS CDC_Operation
                    FROM cdc.Fuente_Clientes_CT;
                END
                ELSE
                BEGIN
                    -- Leer solo los cambios nuevos
                    SELECT 
                        ClienteID, NombreCompleto, Email, Ciudad, Pais, __$operation AS CDC_Operation
                    FROM cdc.fn_cdc_get_all_changes_Fuente_Clientes(@from_lsn, @to_lsn, 'all')
                    ORDER BY __$start_lsn;
                END
                    ", fromLSN, toLSN);
                }
                Dts.Variables["User::LSN_From_Clientes"].Value = query;

                Dts.TaskResult = (int)ScriptResults.Success;

            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "Script CDC", "Error:" + ex.Message, "", 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }
    }

