eration = 4;  -- UPDATE (Before/After)&#xA;&#xA;    COMMIT;&#xA;END TRY&#xA;BEGIN CATCH&#xA;    ROLLBACK;&#xA;    THROW;&#xA;END CATCH;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
