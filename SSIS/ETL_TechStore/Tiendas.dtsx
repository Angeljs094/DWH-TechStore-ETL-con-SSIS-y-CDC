          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{6C3E1113-9381-4030-B69C-C8B3C6E70B0C}"
              SQLTask:SqlStatementSource="BEGIN TRANSACTION;&#xA;BEGIN TRY&#xA;    -------------------------------------------------------------------&#xA;    -- 1) Cerrar versiones actuales que cambiaron&#xA;    -------------------------------------------------------------------&#xA;    UPDATE d&#xA;    SET d.FechaFin = CAST(GETDATE() AS DATE),&#xA;        d.EsActual = 0&#xA;    FROM BDS.DimTienda d&#xA;    INNER JOIN ODS.Tiendas o &#xA;        ON d.TiendaID = o.TiendaID&#xA;    WHERE d.EsActual = 1&#xA;      AND (&#xA;            d.NombreTienda &lt;&gt; o.NombreTienda OR&#xA;            d.Ciudad       &lt;&gt; o.Ciudad OR&#xA;            d.Pais         &lt;&gt; o.Pais OR&#xA;            d.Tipo         &lt;&gt; o.Tipo&#xA;          );&#xA;&#xA;    -------------------------------------------------------------------&#xA;    -- 2) Insertar nuevas versiones (nuevos + modificados)&#xA;    -------------------------------------------------------------------&#xA;    INSERT INTO BDS.DimTienda&#xA;    (&#xA;        TiendaID,&#xA;        NombreTienda,&#xA;        Ciudad,&#xA;        Pais,&#xA;        Tipo,&#xA;        FechaInicio,&#xA;        FechaFin,&#xA;        EsActual&#xA;    )&#xA;    SELECT &#xA;        o.TiendaID,&#xA;        o.NombreTienda,&#xA;        o.Ciudad,&#xA;        o.Pais,&#xA;        o.Tipo,&#xA;        CAST(GETDATE() AS DATE) AS FechaInicio,&#xA;        NULL AS FechaFin,&#xA;        1    AS EsActual&#xA;    FROM ODS.Tiendas o&#xA;    LEFT JOIN BDS.DimTienda d &#xA;        ON o.TiendaID = d.TiendaID AND d.EsActual = 1&#xA;    WHERE d.TiendaID IS NULL&#xA;       OR d.NombreTienda &lt;&gt; o.NombreTienda&#xA;       OR d.Ciudad       &lt;&gt; o.Ciudad&#xA;       OR d.Pais         &lt;&gt; o.Pais&#xA;       OR d.Tipo         &lt;&gt; o.Tipo;&#xA;&#xA;    COMMIT;&#xA;END TRY&#xA;BEGIN CATCH&#xA;    ROLLBACK;&#xA;    THROW;&#xA;END CATCH;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
