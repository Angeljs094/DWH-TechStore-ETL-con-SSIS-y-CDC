          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{6C3E1113-9381-4030-B69C-C8B3C6E70B0C}"
              SQLTask:SqlStatementSource="BEGIN TRANSACTION;&#xA;BEGIN TRY&#xA;    -------------------------------------------------------------------&#xA;    -- 1) Cerrar versiones actuales que cambiaron&#xA;    -------------------------------------------------------------------&#xA;    UPDATE d&#xA;    SET d.FechaFin = CAST(GETDATE() AS DATE),&#xA;        d.EsActual = 0&#xA;    FROM BDS.DimProducto d&#xA;    INNER JOIN ODS.Productos o &#xA;        ON d.ProductoID = o.ProductoID&#xA;    WHERE d.EsActual = 1&#xA;      AND (&#xA;            d.NombreProducto &lt;&gt; o.NombreProducto OR&#xA;            d.Categoria      &lt;&gt; o.Categoria OR&#xA;            d.Marca          &lt;&gt; o.Marca OR&#xA;            d.PrecioLista    &lt;&gt; o.PrecioLista&#xA;          );&#xA;&#xA;    -------------------------------------------------------------------&#xA;    -- 2) Insertar nuevas versiones (nuevos + modificados)&#xA;    -------------------------------------------------------------------&#xA;    INSERT INTO BDS.DimProducto&#xA;    (&#xA;        ProductoID,&#xA;        NombreProducto,&#xA;        Categoria,&#xA;        Marca,&#xA;        PrecioLista,&#xA;        FechaInicio,&#xA;        FechaFin,&#xA;        EsActual&#xA;    )&#xA;    SELECT &#xA;        o.ProductoID,&#xA;        o.NombreProducto,&#xA;        o.Categoria,&#xA;        o.Marca,&#xA;        o.PrecioLista,&#xA;        CAST(GETDATE() AS DATE) AS FechaInicio,&#xA;        NULL AS FechaFin,&#xA;        1    AS EsActual&#xA;    FROM ODS.Productos o&#xA;    LEFT JOIN BDS.DimProducto d &#xA;        ON o.ProductoID = d.ProductoID AND d.EsActual = 1&#xA;    WHERE d.ProductoID IS NULL&#xA;       OR d.NombreProducto &lt;&gt; o.NombreProducto&#xA;       OR d.Categoria      &lt;&gt; o.Categoria&#xA;       OR d.Marca          &lt;&gt; o.Marca&#xA;       OR d.PrecioLista    &lt;&gt; o.PrecioLista;&#xA;&#xA;    COMMIT;&#xA;END TRY&#xA;BEGIN CATCH&#xA;    ROLLBACK;&#xA;    THROW;&#xA;END CATCH;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
